    "version": "0.2.0",
    "configurations": [
        {
            "name": "import",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/import.py",
            "cwd": "${workspaceFolder}",
            "env": {
                "PYTHONPATH": "${workspaceFolder}"
            },
            "console": "integratedTerminal"
        },
        {
            "name": "export",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/export.py",
            "cwd": "${workspaceFolder}",
            "env": {
                "PYTHONPATH": "${workspaceFolder}"
            },
            "console": "integratedTerminal"
        },
        // typings
        {
            "name": "rnastubgen",
            "type": "debugpy",
            "request": "launch",
            "cwd": "${workspaceFolder}",
            "program": "${workspaceFolder}/scripts/rnastubgen.py",
            "args": [
                "typings"
            ],
            "console": "integratedTerminal"
        },
    ]
}